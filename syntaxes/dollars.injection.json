{
	"scopeName": "dollars.injection",
	"injectionSelector": "L:entity.action.yacc",
	"patterns": [
	  {
		"include": "#dollars-keyword"
	  }
	],
	"repository": {
	  "dollars-keyword": {
		"match": "\\$((<[a-zA-Z_*\b]+>)?[0-9]*|(\\$))(\\[[ +\\-*/a-zA-Z_0-9]+\\])?(?=[^$])",
		"name": "keyword.todo"
	  }
	}
 }